                  .space-15
                  .content-row
                    .col-12
                      .content-row.mb-10
                        .col-12.omega
                          span.key N&deg; Consultation
                        .col-12
                          span.colon :
                          span.value # 0000001/18

                    .col-12
                      .content-row.mb-10
                        .col-12.omega
                          span.key Date de Consultation
                        .col-12
                          span.colon :
                          span.value 25/02/2020

